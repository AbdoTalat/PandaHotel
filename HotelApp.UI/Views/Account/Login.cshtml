@model LoginDTO
@{
	Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Panda Hotel Login</title>
	<link rel="stylesheet" href="/Panda/dist/style.css">

</head>
<body>
	<!-- partial:index.partial.html -->
	<!DOCTYPE html>
	<html>
	<head>
		<title>Interative Panda Login</title>
		<!--Google Font-->
		@* <link href="https://fonts.googleapis.com/css?family=Ubuntu:500" rel="stylesheet"> *@
		<!--Jquery CDN-->
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
		<!--Fontawesome CDN-->
		<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
	</head>
	<body style="margin-left:580px">
		<div class="login">
			<form asp-action="Login" asp-controller="Account" method="post">

				@if (TempData["NotActive"] != null)
				{
					<span class="text-danger"> @TempData["NotActive"]</span>
					<br />
				}
				<i class="fa fa-user" aria-hidden="true">&nbsp;&nbsp;</i>
				<input asp-for="email" type="text">
				<br />
				<span asp-validation-for="email" ></span>
				<br>

				<i class="fa fa-unlock-alt" aria-hidden="true">&nbsp;&nbsp;</i>
				<input asp-for="password" type="password">
				<br />
				<span asp-validation-for="password"></span>

				<br />
				<div class="remember-me" >
					<input asp-for="rememberMe"  type="checkbox" style="width:15px">
					<label asp-for="rememberMe" style="cursor:pointer">Remember me</label>
				</div>
				<br>

				<button type="submit" style="cursor: pointer">Login</button>
            </form>
		</div>
		<div class="backg">
			<div class="panda">
				<div class="earl"></div>
				<div class="earr"></div>
				<div class="face">
					<div class="blshl"></div>
					<div class="blshr"></div>
					<div class="eyel">
						<div class="eyeball1"></div>
					</div>
					<div class="eyer">
						<div class="eyeball2"></div>
					</div>
					<div class="nose">
						<div class="line"></div>
					</div>
					<div class="mouth">
						<div class="m">
							<div class="m1"></div>
						</div>
						<div class="mm">
							<div class="m1"></div>
						</div>
					</div>
				</div>

			</div>
		</div>
		<div class="pawl">
			<div class="p1">
				<div class="p2"></div>
				<div class="p3"></div>
				<div class="p4"></div>
			</div>
		</div>
		<div class="pawr">
			<div class="p1">
				<div class="p2"></div>
				<div class="p3"></div>
				<div class="p4"></div>
			</div>
		</div>
		<div class="handl"></div>
		<div class="handr"></div>
	</body>
</html>
<!-- partial -->
<script src="/Panda/dist/script.js"></script>
@section Scripts{
		@await Html.PartialAsync("_ValidationScriptsPartial")
}
</body>
</html>


