	<script>
		(function () {
			function formatShortDate(iso) {
				var d = new Date(iso);
				return d.toLocaleDateString('en-GB', { day: '2-digit', month: 'short' });
			}

			// Initialize dropdown and wire change handler
			$(document).ready(function () {
				// Adjust the field names passed to loadSelect if your endpoint uses different property names
				loadSelect("#roomTypeSelect", "/RoomType/GetRoomTypesJson", "typeName", "id");

				$("#roomTypeSelect").on("change", function () {
					var val = $(this).val();
					var roomTypeId = (val === "" ? null : val); // convert empty string to null
					loadAvailability(roomTypeId);
				});

				// initial load: all room types
				loadAvailability(null);
			});

			async function loadAvailability(roomTypeId) {
				try {
					var url = "/Dashboard/GetAvailabilityRooms";
					if (roomTypeId && roomTypeId !== "") {
						url += "?roomTypeId=" + encodeURIComponent(roomTypeId);
					}

					const res = await fetch(url, { credentials: 'same-origin' });
					if (!res.ok) throw new Error("Network response was not ok: " + res.status);

					const data = await res.json();

					// Support both shapes (camelCase or PascalCase) just in case
					const availability = data.availability || data.Availability || [];
					const roomTypeName = data.roomTypeName || data.RoomTypeName || (data.roomTypeId ? "Room Type" : "All Room Types");

					if (!Array.isArray(availability) || availability.length === 0) {
						$("#availabilityRow").html("<div class='col-md-12'><p class='text-muted'>No availability data found.</p></div>");
						return;
					}

					var html = "";
					availability.forEach(function (day) {
						var dateIso = day.date || day.Date;
						var label = day.label || day.Label || "";
						var available = (day.availableRooms ?? day.availableRooms === 0) ? day.availableRooms : 0;

						// Make label if missing
						if (!label) {
							var d = new Date(dateIso);
							label = d.toLocaleDateString('en-GB', { weekday: 'short' });
						}
						html += `
							<div class="col-md-2 ">
								<ul class="media-list">
									<li class="media">
										<div class="media-body">
											<h3 class="text-semibold ${available === 0 ? 'text-danger' : 'text-success'}">
												${available}
											</h3>
											<div class="text-bold text-size-large">${label}</div>
											<div class="media-annotation">(${formatShortDate(dateIso)})</div>
										</div>
									</li>
								</ul>
							</div>`;
					});

					$("#availabilityRow").html(html);

				} catch (err) {
					console.error("Error loading availability:", err);
					$("#availabilityRow").html("<div class='col-md-12 text-danger'>Error loading availability</div>");
				}
			}
		})();
	</script>

