	<script>
		async function loadRoomOccupancyDashboard() {
			try {
				const response = await fetch('@Url.Action("GetRoomOccupancyDashboard", "Dashboard")');
				const data = await response.json();

				const labels = data.map(r => r.label);
				const occupied = data.map(r => r.occupiedRooms);
				const totalRooms = data.length > 0 ? data[0].totalRooms : 0;

				require.config({
					paths: { echarts: '/assets/js/plugins/visualization/echarts' }
				});

				require(
					['echarts', 'echarts/theme/limitless', 'echarts/chart/bar', 'echarts/chart/line'],
					function (ec, limitless) {
						const chart = ec.init(document.getElementById('basic_columns'), limitless);

						const options = {
							grid: { x: 40, x2: 40, y: 35, y2: 25 },
							tooltip: { trigger: 'axis' },
							legend: { data: ['Occupied Rooms'] },
							xAxis: [{ type: 'category', data: labels }],
							yAxis: [{ type: 'value', max: totalRooms }],
							series: [{
								name: 'Occupied Rooms',
								type: 'bar',
								data: occupied,
								barWidth: 40,
								itemStyle: {
									normal: {
										color: '#007bff',
										label: { show: true, textStyle: { fontWeight: 500 } }
									}
								},
								markLine: { data: [{ type: 'average', name: 'Average' }] }
							}]
						};

						chart.setOption(options);
						window.addEventListener("resize", () => chart.resize());
					}
				);
			} catch (err) {
				console.error("Error loading room occupancy:", err);
			}
		}

		loadRoomOccupancyDashboard();
	</script>

